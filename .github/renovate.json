{
  "extends": ["config:base"],
  "enabledManagers": ["npm", "github-actions"],
  "labels": ["r:{{depName}}", "r:{{depName}}/{{newVersion}}"],
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ]
}
